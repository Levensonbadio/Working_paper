@article{hu_evaluation_2016,
	title = {Evaluation of the {Spatiotemporal} {Dynamics} of {Oxytetracycline} and {Its} {Control} {Effect} {Against} {Citrus} {Huanglongbing} via {Trunk} {Injection}},
